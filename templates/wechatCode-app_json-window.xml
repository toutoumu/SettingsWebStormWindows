<templateSet group="wechatCode-app.json-window">
  <template name="vpages" value="&quot;pages&quot;:[$End$]," description="window: 页面配置" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="End" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="vwindow" value="&quot;window&quot;: {$end$}," description="window: 窗口配置" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="vnavigationBarBackgroundColor" value="&quot;navigationBarBackgroundColor&quot;: &quot;$value$&quot;," description="window: 导航栏背景颜色，如 #000000" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="value" expression="" defaultValue="&quot;#000000&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="vnavigationBarTextStyle" value="&quot;navigationBarTextStyle&quot;:&quot;$black$&quot;," description="window: 导航栏标题颜色，仅支持 black / white" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="black" expression="" defaultValue="&quot;black&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="vnavigationBarTitleText" value="&quot;navigationBarTitleText&quot;:&quot;$End$&quot;" description="window: 导航栏标题文字内容" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="End" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="vnavigationStyle" value="&quot;navigationStyle&quot;:&quot;$End$&quot;" description="window: 导航栏样式，仅支持以下值： default 默认样式 custom 自定义导航栏，只保留右上角胶囊按钮。" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="End" expression="" defaultValue="&quot;default&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="vbackgroundColor" value="&quot;backgroundColor&quot;:&quot;$End$&quot;" description="window: 窗口的背景色" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="End" expression="" defaultValue="&quot;#ffffff&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="vbackgroundColorTop" value="&quot;backgroundColorTop&quot;:&quot;$End$&quot;" description="window: 顶部窗口的背景色，仅 iOS 支持" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="End" expression="" defaultValue="&quot;#ffffff&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="vbackgroundColorBottom" value="&quot;backgroundColorBottom&quot;:&quot;$End$&quot;" description="window: 底部窗口的背景色，仅 iOS 支持" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="End" expression="" defaultValue="&quot;#ffffff&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="venablePullDownRefresh" value="&quot;enablePullDownRefresh&quot;: $End$" description="window: 是否开启全局的下拉刷新。" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="End" expression="" defaultValue="&quot;false&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="vonReachBottomDistance" value="&quot;onReachBottomDistance&quot;:&quot;$End$&quot;" description="window: 页面上拉触底事件触发时距页面底部距离，单位为 px。" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="End" expression="" defaultValue="&quot;50&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="vpageOrientation" value="&quot;pageOrientation&quot;:&quot;$End$&quot;" description="window: 屏幕旋转设置，支持 auto / portrait / landscape" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="End" expression="" defaultValue="&quot;portrait&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="vrequiredBackgroundModes" value="&quot;requiredBackgroundModes&quot;: [&quot;audio&quot;, &quot;location&quot;]" description="window: 申明需要后台运行的能力，类型为数组。目前支持以下项目：  &quot;audio&quot;, &quot;location&quot;" toReformat="false" toShortenFQNames="true" deactivated="true">
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
</templateSet>